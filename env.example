# Environment Variables Example
# Copy this file to .env and fill in your actual values

# ===========================================
# SUPABASE CONFIGURATION
# ===========================================
# Get these values from your Supabase project dashboard
# https://supabase.com/dashboard/project/[your-project]/settings/api

# Supabase Project URL
SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase Anonymous Key (Public)
SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key (Private - for server-side operations)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ===========================================
# APPLICATION CONFIGURATION
# ===========================================

# Application Environment
NODE_ENV=development

# Application Name
APP_NAME=Pelayanan Keswan

# Application Version
APP_VERSION=1.0.0

# Application URL (for production)
APP_URL=https://pelayanan-keswan.vercel.app

# Application Port (for development)
PORT=3000

# ===========================================
# SECURITY CONFIGURATION
# ===========================================

# JWT Secret for token signing
JWT_SECRET=your-jwt-secret-here

# Encryption Key for sensitive data
ENCRYPTION_KEY=your-encryption-key-here

# Session Secret
SESSION_SECRET=your-session-secret-here

# ===========================================
# DATABASE CONFIGURATION
# ===========================================

# Database URL (if using direct connection)
DATABASE_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres

# Database Pool Size
DB_POOL_SIZE=10

# Database Connection Timeout
DB_TIMEOUT=30000

# ===========================================
# CORS CONFIGURATION
# ===========================================

# Allowed Origins (comma-separated)
CORS_ORIGINS=https://pelayanan-keswan.vercel.app,http://localhost:3000

# CORS Credentials
CORS_CREDENTIALS=true

# CORS Methods
CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS

# CORS Headers
CORS_HEADERS=Content-Type,Authorization,X-Requested-With

# ===========================================
# EMAIL CONFIGURATION
# ===========================================

# SMTP Host
SMTP_HOST=smtp.gmail.com

# SMTP Port
SMTP_PORT=587

# SMTP Username
SMTP_USERNAME=your-email@gmail.com

# SMTP Password
SMTP_PASSWORD=your-app-password

# From Email Address
FROM_EMAIL=noreply@pelayanan-keswan.com

# From Name
FROM_NAME=Pelayanan Keswan

# ===========================================
# NOTIFICATION CONFIGURATION
# ===========================================

# Push Notification Service Key
PUSH_NOTIFICATION_KEY=your-push-notification-key

# SMS Service API Key
SMS_API_KEY=your-sms-api-key

# WhatsApp Business API Token
WHATSAPP_TOKEN=your-whatsapp-token

# ===========================================
# FILE UPLOAD CONFIGURATION
# ===========================================

# Upload Directory
UPLOAD_DIR=uploads

# Max File Size (in bytes)
MAX_FILE_SIZE=10485760

# Allowed File Types
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Storage Provider (local, s3, supabase)
STORAGE_PROVIDER=supabase

# ===========================================
# CACHE CONFIGURATION
# ===========================================

# Redis URL (if using Redis)
REDIS_URL=redis://localhost:6379

# Cache TTL (Time To Live in seconds)
CACHE_TTL=3600

# Cache Prefix
CACHE_PREFIX=pelayanan_keswan

# ===========================================
# LOGGING CONFIGURATION
# ===========================================

# Log Level (error, warn, info, debug)
LOG_LEVEL=info

# Log File Path
LOG_FILE=logs/app.log

# Log Max Size
LOG_MAX_SIZE=10m

# Log Max Files
LOG_MAX_FILES=5

# ===========================================
# RATE LIMITING
# ===========================================

# Rate Limit Window (in milliseconds)
RATE_LIMIT_WINDOW=900000

# Rate Limit Max Requests
RATE_LIMIT_MAX=100

# Rate Limit Skip Successful Requests
RATE_LIMIT_SKIP_SUCCESSFUL=false

# ===========================================
# MONITORING CONFIGURATION
# ===========================================

# Sentry DSN (for error tracking)
SENTRY_DSN=your-sentry-dsn

# Google Analytics ID
GA_TRACKING_ID=your-ga-tracking-id

# Mixpanel Token
MIXPANEL_TOKEN=your-mixpanel-token

# ===========================================
# DEVELOPMENT CONFIGURATION
# ===========================================

# Debug Mode
DEBUG=true

# Hot Reload
HOT_RELOAD=true

# Source Maps
SOURCE_MAPS=true

# ===========================================
# PRODUCTION CONFIGURATION
# ===========================================

# Production Database URL
PROD_DATABASE_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres

# Production Redis URL
PROD_REDIS_URL=redis://your-redis-host:6379

# Production CDN URL
PROD_CDN_URL=https://cdn.pelayanan-keswan.com

# ===========================================
# TESTING CONFIGURATION
# ===========================================

# Test Database URL
TEST_DATABASE_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres

# Test Environment
TEST_ENV=true

# Test Data Seed
TEST_SEED=true

# ===========================================
# FEATURE FLAGS
# ===========================================

# Enable Telemedicine
ENABLE_TELEMEDICINE=true

# Enable Notifications
ENABLE_NOTIFICATIONS=true

# Enable Analytics
ENABLE_ANALYTICS=true

# Enable Debug Mode
ENABLE_DEBUG=false

# Enable Maintenance Mode
MAINTENANCE_MODE=false

# ===========================================
# API CONFIGURATION
# ===========================================

# API Base URL
API_BASE_URL=https://api.pelayanan-keswan.com

# API Version
API_VERSION=v1

# API Rate Limit
API_RATE_LIMIT=1000

# API Timeout
API_TIMEOUT=30000

# ===========================================
# THIRD-PARTY INTEGRATIONS
# ===========================================

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Stripe Secret Key
STRIPE_SECRET_KEY=your-stripe-secret-key

# Stripe Publishable Key
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# PayPal Client ID
PAYPAL_CLIENT_ID=your-paypal-client-id

# PayPal Client Secret
PAYPAL_CLIENT_SECRET=your-paypal-client-secret

# ===========================================
# BACKUP CONFIGURATION
# ===========================================

# Backup Schedule (cron expression)
BACKUP_SCHEDULE=0 2 * * *

# Backup Retention Days
BACKUP_RETENTION_DAYS=30

# Backup Storage Provider
BACKUP_STORAGE=s3

# Backup S3 Bucket
BACKUP_S3_BUCKET=pelayanan-keswan-backups

# ===========================================
# HEALTH CHECK CONFIGURATION
# ===========================================

# Health Check Endpoint
HEALTH_CHECK_ENDPOINT=/health

# Health Check Timeout
HEALTH_CHECK_TIMEOUT=5000

# Health Check Interval
HEALTH_CHECK_INTERVAL=30000

# ===========================================
# CUSTOM CONFIGURATION
# ===========================================

# Custom Configuration
CUSTOM_CONFIG={}

# Feature Toggles
FEATURE_TOGGLES={}

# A/B Testing
AB_TESTING=false

# ===========================================
# NOTES
# ===========================================

# 1. Copy this file to .env
# 2. Fill in your actual values
# 3. Never commit .env to version control
# 4. Use different values for development and production
# 5. Keep your secrets secure
# 6. Rotate your keys regularly
# 7. Use environment-specific configurations
